    "typescript": "^5.3.2"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "express": "^4.18.2",
    "open": "^10.0.0",
    "ora": "^8.0.1",
    "socket.io": "^4.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
